{

  "compilerOptions": {
	"target": "ESNEXT",
	"lib": ["ESNEXT", "dom"],
	"module": "ESNEXT",
	"moduleResolution": "node",

	"noImplicitAny": true,
	"esModuleInterop": true,
	"allowSyntheticDefaultImports": true,
	"strict": true,
	"forceConsistentCasingInFileNames": true,

	"resolveJsonModule": true,
	"isolatedModules": true,
	"noEmit": true,
	"strictNullChecks": true
  }
}